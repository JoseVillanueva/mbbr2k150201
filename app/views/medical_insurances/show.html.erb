<%- model_class = MedicalInsurance -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
<!--
  <dt><strong><%= model_class.human_attribute_name(:nom_seguro) %>:</strong></dt>
  <dd><%= @medical_insurance.nom_seguro %></dd>
-->
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @medical_insurance.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:tax_identification) %>:</strong></dt>
  <dd><%= @medical_insurance.tax_identification %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @medical_insurance.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:website) %>:</strong></dt>
  <dd><%= @medical_insurance.website %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @medical_insurance.email %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              medical_insurances_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_medical_insurance_path(@medical_insurance), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              medical_insurance_path(@medical_insurance),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
